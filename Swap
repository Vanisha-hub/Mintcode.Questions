import java.util.*;
public class Swap{
	public static void main(String[] args) {
	    int a=4,b=6,c;
		System.out.println("Before Swapping :");
		System.out.println(a);
		System.out.println(b);
		
		c=a;
		a=b;
		b=c;
		
		System.out.println("After Swapping :");
		System.out.println(a);
		System.out.println(b);
	}
}
